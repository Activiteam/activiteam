{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "max-line-length": {
      "options": [
        120
      ]
    },
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "no-implicit-dependencies": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}